<!--index.wxml-->
<view class="container1">
  <swiper indicator-dots class="swi" indicator-color="rgba(19,206,102,1)" indicator-active-color="#F7BA2A">
    <block wx:for="{{img}}" wx:key="*this">
      <swiper-item class="swi">
        <image src="{{item}}"/>
        <button class="btn" bindtap="start" wx:if="{{ index == img.length - 1 }}">Go >>></button>
      </swiper-item>
    </block>
  </swiper>
</view>
